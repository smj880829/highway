extends ../angularjs

block content
 div(ng-controller='insertCtl')
    form.form-horizontal
      div.form-group
        label.col-md-1.control-label 종류 :
        div.col-md-2
          select.form-control(ng-model="kind")
            option(value="0") 차량
            option(value="1") 바이크
            option(value="2") 재료
            option(value="3") 부품
        label.col-md-1.control-label 모델명 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="name")

      div.form-group
        label.col-md-1.control-label 정찰가격 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="price_marked")
        label.col-md-1.control-label 판매가격 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="price_selling")

      div.form-group
        label.col-md-1.control-label 매입가격 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="price_purchasing")
        label.col-md-1.control-label 수량 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="count")

      div.form-group
        label.col-md-1.control-label 차량번호 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="number")

      br
      div.form-group
        button.col-md-1.btn.btn-default(ng-click='insert_myitem()') 추가
