extends ../angularjs

block content
 div(ng-controller='insertCtl')
    form.form-horizontal
      div.form-group
        label.col-md-1.control-label 종류 :
        div.col-md-2
          select.form-control(ng-model="kind")
            option(value="0") 차량
            option(value="1") 바이크
            option(value="2") 물품
        label.col-md-1.control-label 모델명 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="name")

      div.form-group
        label.col-md-1.control-label 주행거리 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="dist")
        label.col-md-1.control-label 차량번호 :
        div.col-md-2
          input(type='text',class="form-control",ng-model="number")

      br
      div.form-group
        button.col-md-1.btn.btn-default(ng-click='insert_object()') 추가
